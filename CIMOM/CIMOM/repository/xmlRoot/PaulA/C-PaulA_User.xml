<!DOCTYPE CLASS [
]>

<CLASS NAME="PaulA_User">

  <PROPERTY NAME="Login" TYPE="string">
    <QUALIFIER NAME="key" TYPE="boolean">
      <VALUE>TRUE</VALUE>
    </QUALIFIER>
  </PROPERTY>

  <PROPERTY       NAME="RealName"           TYPE="string"  />
  <PROPERTY       NAME="Password"           TYPE="string"  />
  
  <PROPERTY.ARRAY NAME="FaxExtensions"      TYPE="uint16"  />
  
  <PROPERTY       NAME="HomeDirectory"      TYPE="string"  />
  
  <PROPERTY       NAME="LoginShell"         TYPE="string"  />
  <PROPERTY       NAME="WebAccess"          TYPE="boolean" />
  <PROPERTY       NAME="HasLocalHomepage"   TYPE="boolean" />
  
  <PROPERTY       NAME="MailForward"        TYPE="string"  />
  <PROPERTY       NAME="AutoReply"          TYPE="boolean" />
  <PROPERTY       NAME="ReplyText"          TYPE="string"  />
  
  <PROPERTY       NAME="PrivatePopServer"   TYPE="string"  />
  <PROPERTY       NAME="PrivatePopLogin"    TYPE="string"  />
  <PROPERTY       NAME="PrivatePopPassword" TYPE="string"  />
  
  <PROPERTY.ARRAY NAME="MailAliases"        TYPE="string"  />
  <PROPERTY.ARRAY NAME="DistributionLists"  TYPE="string"  />
  
  <PROPERTY.ARRAY NAME="PaulAGroups"        TYPE="string"  />
  <PROPERTY.ARRAY NAME="PaulAPermissions"   TYPE="string"  />
  
  
  <METHOD NAME="Authenticate" TYPE="boolean">
    <PARAMETER NAME="Password" TYPE="string" />
  </METHOD>
  
</CLASS>

